{
  "name": "mmrtl-ui",
  "version": "0.0.1",
  "scripts": {
    "predev": "npm run prepare",
    "dev": "svelte-kit dev --port 8000",
    "prebuild": "npm run prepare",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepare": "npm run vite-workaround && npm run smui-theme",
    "vite-workaround": "node scripts/vite-workaround.js",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@material/theme": "^12.0.0",
    "@material/typography": "^12.0.0",
    "@sveltejs/adapter-node": "^1.0.0-next.0",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.37.2",
    "smui-theme": "^4.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@smui/button": "^4.2.0",
    "@smui/card": "^4.2.0",
    "@smui/form-field": "^4.2.0",
    "@smui/textfield": "^4.2.0",
    "@smui/top-app-bar": "^4.2.0",
    "@types/leaflet": "^1.7.4",
    "@types/rx": "^4.1.2",
    "leaflet": "^1.7.1",
    "rxjs": "^7.3.0"
  }
}
